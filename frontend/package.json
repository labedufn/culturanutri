  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api:migrations": "cd src/api/culturanutri-api && npm run dev:migrate",
    "api:docker:up": "cd src/api/culturanutri-api && npm run dev:docker",
    "api:install": "cd src/api/culturanutri-api && npm install",
    "front:install": "npm install",
    "app:install": "npm run api:install && npm run front:install",
    "app:dev": "concurrently \"npm run dev\" \"sleep 5 && npm run api:docker:up\" \"sleep 10 && npm run api:migrations\" \"sleep 15 && cd src/api/culturanutri-api && npm run dev:api\" ",
    "app:usuario": "cd src/api/culturanutri-api && npm run dev:usuario"
  },
